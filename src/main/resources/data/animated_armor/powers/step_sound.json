{
  "type": "origins:multiple",
  "walking": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:play_sound",
      "sound": "minecraft:entity.iron_golem.step"
    },
    "interval": 10,
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:moving"
        },
        {
          "type": "origins:on_block"
        },
        {
          "type": "origins:sprinting",
          "inverted": true
        },
        {
          "type": "origins:sneaking",
          "inverted": true
        }
      ]
    }
  },
  "sprinting": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:play_sound",
      "sound": "minecraft:entity.iron_golem.step"
    },
    "interval": 7,
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:moving"
        },
        {
          "type": "origins:on_block"
        },
        {
          "type": "origins:sprinting"
        },
        {
          "type": "origins:sneaking",
          "inverted": true
        }
      ]
    }
  },
  "death": {
    "type": "origins:self_action_when_hit",
    "entity_action": {
      "type": "origins:if_else",
      "condition": {
        "type": "origins:health",
        "comparison": ">",
        "compare_to": 0
      },
      "if_action": {
        "type": "origins:play_sound",
